<%- include('headers'); -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    <div>

    <div class="box">
        <% for ( i of NewItem  ) { %>
            <div class=item>
                <input type="checkbox">
                <p>  <%= i %> </p>
            </div> 
        <% } %>

        <form class="box" action="/" method="POST">
            <input type="text" name="actionItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>

    </div>    
<%- include('footer'); -%>